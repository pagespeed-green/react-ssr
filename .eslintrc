{
    "extends": [
      "airbnb",
      "plugin:import/errors",
      "plugin:import/warnings"
    ],
    "plugins": [
      "jsx-a11y",
      "react",
      "react-hooks"
    ],
      "parser": "babel-eslint",
    "parserOptions": {
      "ecmaVersion": 9,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true,
        "experimentalObjectRestSpread": true,
        "impliedStrict": true
      }
    },
    "env": {
      "browser": true,
      "node": true,
      "es6": true,
      "mocha": true
    },
    "rules": {
      "import/default": 2,
      "import/export": 2,
      "import/named": 2,
      "import/namespace": 2,
      "import/no-named-as-default": 0,
      "import/no-unresolved": [2, { "commonjs": true, "amd": true}],
      "no-console": "error",
      "max-len": ["error", { "code": 150, "ignoreComments": true, "ignoreStrings": true }],
      "react-hooks/rules-of-hooks": "error"
    },
    "settings": {
      "import/resolver": {
        "webpack": {
          "config": "webpack.config.dev.js"
        }
      }
    }
}
  